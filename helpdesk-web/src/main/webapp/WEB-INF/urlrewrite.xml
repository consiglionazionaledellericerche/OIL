<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
  <rule>
    <from>/stdl/**</from>
    <to>/$1</to>
  </rule>
  <rule>
    <from>/stdl</from>
    <to>/</to>
  </rule>
  <rule>
    <from>/pdt</from>
    <to>/</to>
  </rule>
  <rule>
    <from>/hd</from>
    <to>/</to>
  </rule>
  <rule>
    <from>/sigla</from>
    <to>/</to>
  </rule>
  <rule>
    <from>/fse</from>
    <to>/</to>
  </rule>
  <rule>
    <from>/fse/**</from>
    <to>/$1</to>
  </rule>
  <rule>
    <from>/pdt/**</from>
    <to>/$1</to>
  </rule>
  <rule>
    <from>/hd/**</from>
    <to>/$1</to>
  </rule>
  <rule>
    <from>/sigla/**</from>
    <to>/$1</to>
  </rule>
  <rule>
    <from>/cert/**</from>
    <to>/$1</to>
  </rule>
  <rule>
    <from>/cert</from>
    <to>/</to>
  </rule>
</urlrewrite>
